  <div class="pcoded-content">
    <div class="row">
      <div class="card-body">
        <%= bootstrap_form_with(model: @manager, local: true) do |f| %>
          <%= f.text_field :company_name, label: "Nom d'entreprise", required: true %>
          <%= f.text_field :email_field, label: "Adresse email", required: true %>
          <%= f.text_field :phone, label: "Telephone", required: true %>
          <%= f.text_field :town, label: "Ville", required: true %>
          <%= f.text_field :street, label: "Rue", required: true %>
          <%= f.number_field :street_nbr, label: "NÂ° de rue", required: true %>
          <%= f.text_field :iban, label: "IBAN", required: true %>
          <%= f.text_field :bic, label: "Bic" %>
          <%= f.submit class: "btn  btn-primary", value:"Ajouter"%>
        <% end %>
      </div>
    </div>
  </div>
</div>
